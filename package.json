{
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "dev": "NODE_ENV=dev nodemon --watch src/ --delay 500ms --exec ts-node src/start.ts",
    "start:dev": "NODE_ENV=dev ts-node-dev --respawn --transpile-only src/start.ts",
    "start1": "NODE_ENV=production node build/start.js",
    "watch": "tsc --watch",
    "start" :"ts-node-dev"
  },
  "name": "typescript_blockchain",
  "version": "1.0.0",
  "dependencies": {
    "tsc-watch": "^6.0.0",
    "typescript": "^5.0.2"
  },
  "devDependencies": {
    "ts-node": "^10.9.1"
  }
}
